{
  "name": "blog",
  "version": "0.3.0",
  "description": "A starter kit for a gh-pages Aurelia Blog.",
  "repository": "git+ssh://git@github.com/SteveHartzog/blog.git",
  "keywords": [
    "aurelia",
    "gh-pages",
    "blog",
    "webpack"
  ],
  "author": "Steve Hartzog <shartzog@gmail.com> (http://stevehartzog.github.io/)",
  "license": "CC0-1.0",
  "contributors": [],
  "bugs": {
    "url": "https://github.com/SteveHartzog/blog/issues"
  },
  "homepage": "https://github.com/SteveHartzog/blog#readme",
  "scripts": {
    "start": "nps",
    "test": "nps test"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "\\.spec\\.(ts|js)x?$",
    "setupFiles": [
      "<rootDir>/test/jest-pretest.ts"
    ],
    "testEnvironment": "node",
    "moduleNameMapper": {
      "aurelia-(.*)": "<rootDir>/node_modules/$1"
    }
  },
  "devDependencies": {
    "@firebase/app": "^0.1.5",
    "@firebase/auth": "^0.3.1",
    "@firebase/database": "^0.1.6",
