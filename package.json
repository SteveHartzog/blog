    "@types/lodash": "^4.14.104",
    "@types/markdown-it": "0.0.4",
    "@types/node": "^9.4.6",
    "@types/webpack": "^3.0.4",
    "aurelia-cli": "^0.32.0",
    "aurelia-loader-nodejs": "^1.0.1",
    "aurelia-pal-nodejs": "1.0.0-beta.3.0.0",
    "aurelia-testing": "^1.0.0-beta.4.0.0",
    "aurelia-tools": "^1.0.0",
    "awesome-typescript-loader": "^3.4.1",
    "aurelia-webpack-plugin": "2.0.0-rc.5",
    "copy-webpack-plugin": "4.3.1",
    "cross-env": "5.1.3",
    "css-loader": "^0.28.8",
    "del": "^3.0.0",
    "expose-loader": "^0.7.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-rename": "^1.2.2",
    "gulp-util": "^3.0.8",
    "html-loader": "^0.5.4",
    "html-webpack-plugin": "2.30.1",
    "http-server": "0.10.0",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "2.8.0",
    "jest": "^22.0.5",
    "jest-cli": "^22.0.5",
    "json-loader": "^0.5.7",
    "minimatch": "^3.0.2",
    "node-sass": "^4.7.2",
    "nps": "^5.7.1",
